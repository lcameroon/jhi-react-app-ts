{
  "name": "cra-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:dev mockapi",
    "prestart:dev": "npm run i18n",
    "start:dev": "react-scripts start",
    "mockapi": "json-server --watch mockapi/router.js --port 8080 --delay 500 --routes mockapi/rewriter.json --middlewares mockapi/middleware.js",
    "----------- BUILD ------------------": "",
    "prebuild": "npm run i18n",
    "build": "react-scripts build",
    "build:suite": "npm run test && npm run build",
    "----------- TEST --------------------": "",
    "test": "react-scripts test --env=jsdom --coverage",
    "tdd": "react-scripts test --env=jsdom --watchAll",
    "test:clearCache": "node ./node_modules/jest/bin/jest.js --clearCache",
    "----------- VERSION ----------------": "",
    "prerelease": "npm version patch --no-git-tag-version",
    "release": "VERSION=$(node -pe \"require('./package.json').version\") && git add . && git commit -m 'Bump version' --no-verify && git tag -a v$VERSION -m 'v$VERSION'",
    "postrelease": "git push --follow-tags --no-verify",
    "----------- MISC -------------------": "",
    "i18n": "node ./i18n.js",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "analyze": "source-map-explorer build/static/js/main.*",
    "reinstall": "rm -rf ./node_modules && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "availity-reactstrap-validation": "^2.6.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.8",
    "react-infinite-scroller": "^1.2.4",
    "react-jhipster": "^0.9.0",
    "react-redux": "^7.1.0",
    "react-redux-loading-bar": "^4.3.0",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.3.2",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.0.1",
    "@types/redux": "^3.6.0",
    "json-server": "^0.15.0",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "3.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "source-map-explorer": "^2.0.1",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.3"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
