{
  "name": "react-app-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently 'npm run scss:watch' 'npm run start:dev'",
    "start:dev": "react-scripts-ts start",
    "start:mock": "concurrently 'npm start' 'npm run mock:server'",
    "scss:watch": "npm run build:scss -- --watch --recursive",
    "mock:server": "json-server mockapi/server.js --port 8080 --delay 500 --routes mockapi/routes.json --middlewares mockapi/middleware.js",
    "----------- BUILD ------------------": "",
    "build": "npm run build:scss && react-scripts-ts build",
    "build:suite": "npm run test && npm run build",
    "build:scss": "node-sass-chokidar src/assets/scss/styles.scss -o src/assets",
    "----------- TEST --------------------": "",
    "test": "CI=true react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom --watchAll",
    "test:clearCache": "node ./node_modules/jest/bin/jest.js --clearCache",
    "----------- MISC -------------------": "",
    "lint": "tslint --project ./tsconfig.json",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm run test",
    "postinstall": "node ./node_modules/husky/bin/install.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.0",
    "@fortawesome/free-solid-svg-icons": "5.1.0",
    "@fortawesome/react-fontawesome": "0.1.0",
    "availity-reactstrap-validation": "2.0.2",
    "axios": "0.18.0",
    "bootstrap": "4.1.1",
    "loaders.css": "0.1.2",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.3",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-redux-loading-bar": "4.0.5",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-toastify": "4.1.0",
    "react-transition-group": "2.4.0",
    "reactstrap": "6.2.0",
    "redux": "4.0.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/lodash": "4.14.110",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "6.0.3",
    "@types/react-router-dom": "4.2.7",
    "@types/redux": "3.6.31",
    "@types/selenium-webdriver": "3.0.10",
    "@types/webpack-env": "1.13.6",
    "concurrently": "^3.6.0",
    "cross-env": "5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "husky": "0.14.3",
    "json-server": "^0.14.0",
    "node-sass-chokidar": "^1.3.4",
    "numeral": "^2.0.6",
    "prettier": "1.13.7",
    "protractor": "^5.4.1",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.6.0",
    "redux-mock-store": "1.5.3",
    "source-map-explorer": "^1.6.0",
    "ts-loader": "^3.5.0",
    "tslib": "1.9.3",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-eslint-rules": "5.3.1",
    "typescript": "^3.1.6"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080",
      "secure": false
    }
  }
}
